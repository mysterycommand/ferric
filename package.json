{
  "name": "ferric",
  "version": "1.0.0",
  "description": "Rusty physics",
  "repository": "git@github.com:mysterycommand/ferric.git",
  "author": "Matt Hayes <matt@mysterycommand.com>",
  "license": "(MIT OR Apache-2.0)",
  "private": true,
  "engines": {
    "node": "10.16.3",
    "npm": "6.11.2",
    "yarn": "1.17.3"
  },
  "scripts": {
    "reset": "yarn clean node_modules/ && yarn install",
    "clean": "cargo clean && rm -rf .cache/ coverage/ dist/ pkg/",
    "start": "parcel src/index.html",
    "build": "parcel build src/index.html --public-url=./",
    "serve": "yarn build && npx serve dist",
    "lint": "tslint 'src/**/*.ts' && cargo fmt --all -- --check && cargo clippy",
    "test": "cargo build && cargo test"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@babel/preset-env": "7.6.3",
    "@babel/preset-typescript": "7.6.0",
    "@semantic-release/changelog": "3.0.4",
    "@semantic-release/commit-analyzer": "6.3.0",
    "@semantic-release/git": "7.0.16",
    "@semantic-release/github": "5.5.3",
    "@semantic-release/npm": "5.2.0",
    "@semantic-release/release-notes-generator": "7.3.0",
    "@types/node": "12.12.0",
    "commitizen": "4.0.3",
    "cz-conventional-changelog": "3.0.2",
    "parcel-bundler": "1.12.3",
    "parcel-plugin-wasm-pack": "4.1.2",
    "prettier": "1.18.2",
    "semantic-release": "15.13.24",
    "semantic-release-github-pr": "https://github.com/mysterycommand/semantic-release-github-pr.git#fix/34-and-35",
    "tslint": "5.20.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.6.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
